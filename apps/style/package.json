{
  "name": "@acme/style",
  "private": true,
  "type": "module",
  "main": "src/main.scss",
  "scripts": {
    "build": "bun run scripts/build.ts",
    "build:watch": "bun run scripts/watch.ts",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "preview": "serve dist --listen 5173 --single --no-request-logging --no-clipboard",
    "dev": "concurrently --kill-others --names watch,serve -r \"bun run build:watch\" \"bun run preview\"",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint --flag unstable_native_nodejs_ts_config",
    "typecheck": "tsc --noEmit",
    "check:spelling": "cspell -c .cspell.json --no-progress --no-summary --no-must-find-files --unique .",
    "with-env": "dotenv -e ../../.env --"
  },
  "peerDependencies": {
    "typescript": "catalog:"
  },
  "dependencies": {
    "sass-embedded": "^1.93.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/cspell-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@types/serve-handler": "^6.1.4",
    "commander": "^14.0.1",
    "concurrently": "^9.2.1",
    "eslint": "catalog:",
    "cspell": "catalog:",
    "ora": "^9.0.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-load-config": "^6.0.1",
    "postcss-preset-env": "^10.4.0",
    "prettier": "catalog:",
    "serve": "^14.2.5",
    "serve-handler": "^6.1.6"
  },
  "prettier": "@acme/prettier-config"
}
